<aura:component controller="SearchContactlist" implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction" access="global" >
   <aura:attribute name="conlist" type="list" />
    <aura:handler name="init" value="{!this}" action="{!c.doinit}" />
     <aura:attribute name="contacts" type="Contact" />
      <aura:attribute name="columns" type="list" />
    
    <lightning:select name="select1" label="choose An contact Name" aura:id="distance" onchange="{!c.onChange}">
        <option value="null">None</option>
        <aura:iteration items="{!v.conlist}" var="item">
         <option value="{!item.Id}"> {!item.Name}</option>
        </aura:iteration>
    
    </lightning:select>
     <lightning:card iconName="standard:contact" title="Contact list">
        <lightning:datatable data="{!v.contacts}" columns="{!v.columns}" keyField="Id" />
    
    </lightning:card>
    
	
</aura:component>